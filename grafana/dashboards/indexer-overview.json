{
  "dashboard": {
    "title": "Indexer-RS Overview",
    "tags": ["indexer", "blockchain"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Listener - Blocks Synced",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(listener_blocks_synced_total[5m])",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Blocks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Listener - Logs Fetched",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(listener_logs_fetched_total[5m])",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Logs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Listener - Current Block",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "listener_current_block",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Block Number"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Listener - Blocks Behind",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "listener_blocks_behind",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Blocks"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Listener - RPC Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(listener_rpc_errors_total[5m])",
            "legendFormat": "{{address}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Listener - Reorgs Detected",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(listener_reorgs_detected_total[5m])",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Reorgs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Processor - Logs Processed",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(processor_logs_processed_total[5m])",
            "legendFormat": "{{contract}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Logs/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Processor - Unprocessed Logs",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "processor_unprocessed_logs",
            "legendFormat": "Chain {{chain_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Processor - Processing Errors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "rate(processor_logs_errors_total[5m])",
            "legendFormat": "{{contract}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Processor - Processing Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(processor_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{contract}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(processor_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{contract}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Listener - RPC Request Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(listener_rpc_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{method}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(listener_rpc_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{method}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Listener - Logs Saved",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "rate(listener_logs_saved_total[5m])",
            "legendFormat": "{{address}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Logs/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {}
  }
}

